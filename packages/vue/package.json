{
  "name": "pvue",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && rollup -c && tsc -p tsconfig.json"
  },
  "sideEffects": false,
  "dependencies": {
    "@pure-vue/compiler-core": "workspace:*",
    "@pure-vue/compiler-dom": "workspace:*",
    "@pure-vue/runtime-core": "workspace:*",
    "@pure-vue/runtime-dom": "workspace:*",
    "@pure-vue/reactivity": "workspace:*",
    "@pure-vue/shared": "workspace:*"
  }
}
